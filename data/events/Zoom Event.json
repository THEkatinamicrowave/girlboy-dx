{
    "params": [
        {
            "name": "Camera Focus Position (formatted as \"X,Y\")",
            "type": "String",
            "defaultValue": "0,0"
        },
        {
            "name": "Zoom Amount",
            "type": "Float(0, 3, 1, 3)",
            "defaultValue": 1.000
        },
        {
            "name": "Fade Duration (in steps)",
            "type": "Int(0, 9999)",
            "defaultValue": 4
        },
        {
            "name": "Zoom Tween Type",
            "type": "DropDown('back', 'bounce', 'circ', 'cube', 'elastic', 'expo', 'linear', 'quad', 'quart', 'quint', 'sine', 'smoothStep', 'smootherStep')",
            "defaultValue": "linear"
        },
        {
            "name": "Zoom Tween Direction",
            "type": "DropDown('In', 'Out', 'InOut')",
            "defaultValue": "InOut"
        },
        {
            "name": "Reset Cam Zoom and Position?",
            "type": "Bool",
            "defaultValue": false
        }
    ]
}